{
  "python.pythonPath": "python3",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "explorer.width": 30,
  "explorer.floating.border.title": "File Explorer",
  "explorer.icon.enableNerdfont": true,
  "explorer.toggle": false,
  "explorer.buffer.child.template": "[selection | 1] [bufnr] [name][readonly] [modified] ",
  "explorer.file.root.template": "[icon] [title] [root]",
  "explorer.file.child.template": "[selection & 1 | clip | git | 2] [indent][icon | 1] [filename omitCenter 1] [diagnosticError & 1][readonly] [linkIcon & 1][link growRight 1 omitCenter 5][size]",
  "explorer.git.icon.status.modified": "",
  "explorer.git.icon.status.ignored": "",
  "explorer.git.icon.status.untracked": "",
  "explorer.git.icon.status.deleted": "",
  "explorer.filename.colored.enable": false,
  "explorer.keyMappings.global": {
    "v": "open:vsplit",
    "f": false,
    "F": false,
    "l": false,
    "h": false,
    "q": ["nowait", "quit"],
    "<space>": ["wait", "expanded?", "collapse", "expand"]
  },
  "explorer.sources": [
    {
      "name": "bookmark",
      "expand": false
    },
    {
      "name": "buffer",
      "expand": true
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "coc.preferences.extensionUpdateCheck" : "daily",
  "coc.preferences.formatOnType" : true,
  "suggest.maxCompleteItemCount": 10,
  "suggest.minTriggerInputLength": 2,
  "suggest.completionItemKindLabels": {
    "text": "text",
    "method": "method",
    "function": "func",
    "constructor": "constructor",
    "field": "field",
    "variable": "var",
    "class": "class",
    "interface": "interface",
    "module": "module",
    "property": "property",
    "unit": "unit",
    "value": "value",
    "enum": "enum",
    "keyword": "keyword",
    "snippet": "snip",
    "color": "color",
    "file": "file",
    "reference": "ref",
    "folder": "folder",
    "enumMember": "enum",
    "constant": "const",
    "struct": "struct",
    "event": "event",
    "operator": "operator",
    "typeParameter": "type",
    "default": "default"
  }
}

